<?xml version="1.0" encoding="utf-8"?>
<changeBounds xmlns:android="http://schemas.android.com/apk/res/android"
    android:duration="500">
  <arcMotion
      android:interpolator="@android:interpolator/accelerate_decelerate"
      android:minimumHorizontalAngle="15"
      android:maximumAngle="90"/>
  <changeTransform android:interpolator="@android:interpolator/accelerate_decelerate"/>
  <changeClipBounds android:interpolator="@android:interpolator/accelerate_decelerate"/>
  <changeImageTransform android:interpolator="@android:interpolator/accelerate_decelerate"/>
</changeBounds>